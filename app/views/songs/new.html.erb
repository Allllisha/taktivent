<div class ="background-home">
<div class="center-container">
<div class="form-container">
  <%= simple_form_for [@event, @song] do |f| %>
    <%= f.input :name, class: "input" %>
    <%= f.input :composer_name, class: "input" %>
    <%= f.input :start_at, class: "input" %>
    <%= f.input :length_in_minute, class: "input" %>
    <%= f.input :description, class: "input" %>
    <%= f.input :images, as: :file, input_html: { multiple: true }, class: "input" %>
    <%= f.button :submit, class: "input", class: "btn-outline-secondary" %>
  <% end %>
  </div>
</div>  
</div>